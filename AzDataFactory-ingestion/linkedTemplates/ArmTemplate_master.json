{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AzDataFactory-ingestion"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"Hdfs1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Hdfs1'"
		},
		"OnPremWPG02KQP5_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'OnPremWPG02KQP5'"
		},
		"OnPremWPG02KQP5_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'OnPremWPG02KQP5'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://bharatdatalakestore.dfs.core.windows.net"
		},
		"Hdfs1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://20.67.107.211/"
		},
		"Hdfs1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "vermabha"
		},
		"OnPremWPG02KQP5_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "AP\\vermabha"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AzDataFactory-ingestion_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"Hdfs1_password": {
						"value": "[parameters('Hdfs1_password')]"
					},
					"OnPremWPG02KQP5_connectionString": {
						"value": "[parameters('OnPremWPG02KQP5_connectionString')]"
					},
					"OnPremWPG02KQP5_password": {
						"value": "[parameters('OnPremWPG02KQP5_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"Hdfs1_properties_typeProperties_url": {
						"value": "[parameters('Hdfs1_properties_typeProperties_url')]"
					},
					"Hdfs1_properties_typeProperties_userName": {
						"value": "[parameters('Hdfs1_properties_typeProperties_userName')]"
					},
					"OnPremWPG02KQP5_properties_typeProperties_userName": {
						"value": "[parameters('OnPremWPG02KQP5_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}